{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "plugins": [
        "ejs"
    ],
    "rules": {
        "indent": ["error", 4]
    }
}
